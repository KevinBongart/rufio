<% @events.each do |event| %>
  <%= div_for event, class: event.event_type, data: { customerid: event.customer_id } do %>
    <%= render "events/#{event.event_type}", event: event %>
  <% end %>
<% end %>

<div id="pagination">
  <div><%= page_entries_info @events %></div>
  <%= will_paginate @events %>
</div>
